{
  "version": 2,
  "cors": {
    "allowOrigins": "*",
    "allowMethods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
    "allowHeaders": ["Origin", "X-Requested-With", "Content-Type", "Accept"]
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
