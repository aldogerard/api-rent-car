{
  "version": 2,
  "edge": {
    "cors": {
      "origin": "*",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
      "headers": ["Origin", "X-Requested-With", "Content-Type", "Accept"]
    }
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
